<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragment.FeedbackFragment"
    android:background="@drawable/bg_login">

    <RelativeLayout
        android:id="@+id/rl_back_app_info"
        android:layout_width="match_parent"
        android:layout_height="@dimen/space_50dp">

        <ImageButton
            android:id="@+id/ib_back_app_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/space_10dp"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_back_cold_32dp" />
    </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/rl_back_app_info"
            android:src="@drawable/info_app"/>

</RelativeLayout>